{
  "name": "react-fundamentals",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "license": "MIT",
  "scripts": {
    "gulp": "node -r ts-node/register node_modules/gulp/bin/gulp",
    "prestart": "npm run gulp -- server:prestart",
    "start": "npm run server",
    "clean": "npm run gulp -- clean",
    "test": "npm run eslint && npm run tscheck && npm run coverage",
    "test:r": "jest",
    "test:w": "jest --watch",
    "eslint:fix": "eslint src --ext \"tsx\" --fix",
    "eslint": "npm run gulp eslint",
    "eslint:w": "npm run gulp eslint:watch",
    "tscheck": "echo tscheck... && tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "tsclint": "tsc --noEmit --pretty false --strictNullChecks --forceConsistentCasingInFileNames --noImplicitReturns --noImplicitThis --noUnusedLocals",
    "tsclint:w": "npm run tsclint -- --watch",
    "coverage": "jest --collectCoverage --coverageDirectory=.testresults/coverage",
    "server": "webpack-dev-server --progress --hot --colors",
    "server:prod": "npm run server -- --env.prod",
    "build": "webpack",
    "build:prod": "npm run build -- --env.prod",
    "build:libs": "npm run build -- --env.libs --progress",
    "build:style": "npm run build -- --env.style --progress",
    "build:style:prod": "npm run build -- --env.style --env.prod",
    "build:vendors": "npm run build:libs && npm run build:style",
    "dev2a": "concurrently -q -- \"npm run test:w\" \"npm run gulp eslint:watch\"",
    "dev3a": "concurrently -q -- \"npm run test:w\" \"npm run gulp eslint:watch\" \"npm run tscheck:w\"",
    "dev4a": "concurrently -q -- \"npm run test:w\" \"npm run gulp eslint:watch\" \"npm run tscheck:w\" \"npm run server\"",
    "dev2b": "concurrently -q -- \"npm run test:w\" \"npm run server\"",
    "dev2c": "concurrently -q -- \"npm run tscheck:w\" \"npm run start\"",
    "dev2d": "concurrently -q -- \"npm run eslint:w\" \"npm run tsclint:w\"",
    "serve": "npm run prestart && npm run dev4a",
    "prepush": "npm run -q test"
  },
  "dependencies": {
    "njct": "3.1.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": ".*\\.spec\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx"
    ]
  },
  "devDependencies": {
    "1-liners": "0.4.0",
    "@blueprintjs/core": "1.35.0",
    "@types/jest": "21.1.9",
    "@types/node": "8.5.2",
    "@types/react": "16.0.31",
    "@types/react-dom": "16.0.3",
    "@types/react-test-renderer": "16.0.0",
    "@types/webpack": "3.8.1",
    "@types/webpack-env": "1.13.3",
    "add-asset-html-webpack-plugin": "2.1.2",
    "autoprefixer": "7.2.3",
    "awesome-typescript-loader": "3.4.1",
    "bourbon": "4.3.4",
    "circular-dependency-plugin": "4.3.0",
    "cross-spawn": "5.1.0",
    "css-loader": "0.28.7",
    "del": "3.0.0",
    "ejs-loader": "0.3.0",
    "eslint": "4.13.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.5.1",
    "eslint-plugin-tslint": "2.1.0",
    "eslint-plugin-typescript": "0.8.1",
    "eslint-plugin-unicorn": "3.0.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "glob": "7.1.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-eslint": "4.0.0",
    "gulp-if": "2.0.2",
    "gulp-ignore": "2.0.2",
    "gulp-load-plugins": "1.5.0",
    "gulp-memory-cache": "0.3.0",
    "gulp-util": "3.0.8",
    "html-loader": "0.5.1",
    "html-webpack-plugin": "2.30.1",
    "is-changed": "2.1.6",
    "jest": "22.0.4",
    "node-sass": "4.7.2",
    "postcss": "6.0.14",
    "postcss-flexbox": "1.0.3",
    "postcss-import": "11.0.0",
    "postcss-loader": "2.0.9",
    "postcss-url": "7.3.0",
    "react-hot-loader": "3.1.3",
    "react-test-renderer": "16.2.0",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.1",
    "to-boolean": "1.0.0",
    "ts-jest": "22.0.0",
    "ts-node": "4.1.0",
    "tslib": "1.8.1",
    "tslint": "5.8.0",
    "typescript": "2.6.2",
    "typescript-eslint-parser": "11.0.0",
    "url-loader": "0.6.2",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.9.7",
    "webpack-hot-middleware": "2.21.0"
  }
}
