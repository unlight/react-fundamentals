{
  "name": "react-typescript-starter",
  "version": "0.0.0-dev",
  "private": true,
  "description": "",
  "license": "MIT",
  "scripts": {
    "gulp": "node -r ts-node/register/transpile-only node_modules/gulp/bin/gulp",
    "start": "npm run dev",
    "dev": "webpack-dev-server --hot --colors",
    "predev": "npm run gulp -- preserver",
    "clean": "npm run gulp -- clean",
    "test": "npm run eslint && npm run tscheck && npm run test:cov",
    "test:r": "jest --runInBand --verbose",
    "test:w": "jest --runInBand --watch",
    "test:cov": "jest --verbose --runInBand --collectCoverage",
    "test:d": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand",
    "eslint:fix": "eslint src --ext \"tsx\" --fix",
    "eslint": "npm run gulp -- eslint",
    "eslint:w": "npm run gulp -- eslint:w",
    "tscheck": "echo tscheck... && tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "tsclint": "tsc --noEmit --pretty false --strict --forceConsistentCasingInFileNames --noImplicitReturns --noImplicitThis --noUnusedLocals",
    "tsclint:w": "npm run tsclint -- --watch",
    "build": "webpack",
    "build:prod": "npm run build -- --env.prod",
    "build:libs": "npm run build -- --env.libs --progress",
    "build:style": "npm run build -- --env.style --progress",
    "build:style:prod": "npm run build -- --env.style --env.prod",
    "build:vendors": "npm run build:libs && npm run build:style",
    "prepush": "npm run -q test"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "njct": "7.1.3",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-eventmanager": "0.0.3",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1"
  },
  "devDependencies": {
    "1-liners": "0.4.0",
    "@blueprintjs/core": "3.11.0",
    "@types/classnames": "2.2.7",
    "@types/execa": "0.9.0",
    "@types/gulp": "4.0.5",
    "@types/jest": "23.3.12",
    "@types/node": "10.12.18",
    "@types/react": "16.7.18",
    "@types/react-dom": "16.0.11",
    "@types/react-test-renderer": "16.0.3",
    "@types/rimraf": "2.0.2",
    "@types/webpack": "4.4.22",
    "@types/webpack-dev-server": "3.1.1",
    "@types/webpack-env": "1.13.6",
    "add-asset-html-webpack-plugin": "3.1.2",
    "autoprefixer": "9.4.5",
    "circular-dependency-plugin": "5.0.2",
    "condition-loader": "1.0.5",
    "css-loader": "2.1.0",
    "eslint": "5.12.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.3",
    "eslint-plugin-tslint": "3.1.0",
    "eslint-plugin-typescript": "0.14.0",
    "eslint-plugin-unicorn": "6.0.1",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "3.0.1",
    "glob": "7.1.3",
    "gulp": "4.0.0",
    "gulp-eslint": "5.0.0",
    "gulp-if": "2.0.2",
    "gulp-ignore": "2.0.2",
    "gulp-load-plugins": "1.5.0",
    "gulp-memory-cache": "0.3.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "is-changed": "2.2.0",
    "jest": "23.6.0",
    "node-sass": "4.11.0",
    "postcss": "7.0.11",
    "postcss-flexbox": "1.0.3",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-url": "8.0.0",
    "react-hot-loader": "4.6.3",
    "react-test-renderer": "16.7.0",
    "rimraf": "2.6.3",
    "sass-loader": "7.1.0",
    "source-map-loader": "0.2.4",
    "style-loader": "0.23.1",
    "ts-jest": "23.10.5",
    "ts-loader": "5.3.3",
    "ts-node": "7.0.1",
    "tslib": "1.9.3",
    "tslint": "5.12.1",
    "typescript": "3.2.2",
    "typescript-eslint-parser": "21.0.2",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "1.1.2",
    "webpack": "4.28.4",
    "webpack-cli": "3.2.1",
    "webpack-dev-server": "3.1.14",
    "webpack-remove-assets-plugin": "1.0.1"
  }
}
