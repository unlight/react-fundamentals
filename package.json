{
  "name": "react-fundamentals",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "license": "MIT",
  "scripts": {
    "gulp": "node -r ts-node/register node_modules/gulp/bin/gulp",
    "prestart": "npm run gulp -- server:prestart",
    "start": "npm run server",
    "dev": "npm run server",
    "clean": "npm run gulp -- clean",
    "test": "npm run eslint && npm run tscheck && npm run test:r",
    "test:r": "jest --verbose",
    "test:w": "jest --watch",
    "test:cov": "jest --verbose --runInBand --collectCoverage --coverageDirectory=.testresults/coverage",
    "test:d": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand",
    "eslint:fix": "eslint src --ext \"tsx\" --fix",
    "eslint": "npm run gulp eslint",
    "eslint:w": "npm run gulp eslint:w",
    "tscheck": "echo tscheck... && tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "tsclint": "tsc --noEmit --pretty false --strictNullChecks --forceConsistentCasingInFileNames --noImplicitReturns --noImplicitThis --noUnusedLocals",
    "tsclint:w": "npm run tsclint -- --watch",
    "server": "webpack-dev-server --hot --colors",
    "server:prod": "npm run server -- --env.prod",
    "build": "webpack",
    "build:prod": "npm run build -- --env.prod",
    "build:libs": "npm run build -- --env.libs --progress",
    "build:style": "npm run build -- --env.style --progress",
    "build:style:prod": "npm run build -- --env.style --env.prod",
    "build:vendors": "npm run build:libs && npm run build:style",
    "test:eslint": "concurrently -q -- \"npm run test:w\" \"npm run gulp eslint:w\"",
    "test:eslint:tscheck": "concurrently -q -- \"npm run test:w\" \"npm run gulp eslint:w\" \"npm run tscheck:w\"",
    "dev:test:eslint:tscheck": "concurrently -q -- \"npm run test:w\" \"npm run gulp eslint:w\" \"npm run tscheck:w\" \"npm run server\"",
    "dev:test": "concurrently -q -- \"npm run test:w\" \"npm run server\"",
    "dev:tscheck": "concurrently -q -- \"npm run tscheck:w\" \"npm run start\"",
    "dev:tsclint": "concurrently -q -- \"npm run eslint:w\" \"npm run tsclint:w\"",
    "serve": "npm run prestart && npm run dev:test:eslint:tscheck",
    "prepush": "npm run -q test"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "njct": "3.1.0",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-eventmanager": "0.0.3",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2"
  },
  "devDependencies": {
    "1-liners": "0.4.0",
    "@blueprintjs/core": "2.2.1",
    "@types/classnames": "2.2.3",
    "@types/gulp": "4.0.5",
    "@types/jest": "22.2.3",
    "@types/node": "9.6.6",
    "@types/react": "16.3.12",
    "@types/react-dom": "16.0.5",
    "@types/react-test-renderer": "16.0.1",
    "@types/webpack": "4.1.3",
    "@types/webpack-env": "1.13.6",
    "add-asset-html-webpack-plugin": "3.0.0-0",
    "autoprefixer": "8.3.0",
    "bourbon": "5.0.0",
    "circular-dependency-plugin": "5.0.2",
    "cross-spawn": "6.0.5",
    "css-loader": "0.28.11",
    "del": "3.0.0",
    "ejs-loader": "0.3.1",
    "eslint": "4.19.1",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "eslint-plugin-tslint": "2.1.0",
    "eslint-plugin-typescript": "0.11.0",
    "eslint-plugin-unicorn": "4.0.3",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.11",
    "glob": "7.1.2",
    "gulp": "4.0.0",
    "gulp-eslint": "4.0.2",
    "gulp-if": "2.0.2",
    "gulp-ignore": "2.0.2",
    "gulp-load-plugins": "1.5.0",
    "gulp-memory-cache": "0.3.0",
    "gulp-util": "3.0.8",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "is-changed": "2.1.6",
    "jest": "22.4.3",
    "node-sass": "4.8.3",
    "postcss": "6.0.21",
    "postcss-flexbox": "1.0.3",
    "postcss-import": "11.1.0",
    "postcss-loader": "2.1.4",
    "postcss-url": "7.3.2",
    "react-hot-loader": "4.1.1",
    "react-test-renderer": "16.3.2",
    "sass-loader": "7.0.1",
    "style-loader": "0.21.0",
    "to-boolean": "1.0.0",
    "ts-jest": "22.4.4",
    "ts-loader": "4.2.0",
    "ts-node": "5.0.1",
    "tslib": "1.9.0",
    "tslint": "5.9.1",
    "typescript": "2.8.3",
    "typescript-eslint-parser": "15.0.0",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "1.0.1",
    "webpack": "4.6.0",
    "webpack-cli": "2.0.15",
    "webpack-dev-server": "3.1.3",
    "webpack-hot-middleware": "2.22.1",
    "webpack-loader-helper": "1.0.0"
  }
}
